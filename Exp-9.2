name: CI/CD Pipeline
on:
  push:
    branches:
      - main
jobs:
  build:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout code
        uses: actions/checkout@v4
      - name: Set up Node.js
        uses: actions/setup-node@v4
        with:
          node-version: 18
      - name: Install dependencies
        run: npm install
      - name: Build React app
        run: npm run build
git init
git add .
git commit -m "Dockerized React app with CI/CD pipeline"
git branch -M main
git remote add origin https://github.com/yourusername/react-docker-ci-cd.git
git push -u origin main
